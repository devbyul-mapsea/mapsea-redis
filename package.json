{
  "name": "proxy-template-express",
  "version": "1.0.0",
  "description": "mapsea proxy server tamplate created using express.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_MODE=production ts-node src/index.ts",
    "dev": "npm run swagger & NODE_MODE=developer nodemon --exec ts-node --watch src --delay 1 src/index.ts",
    "local": "npm run swagger & NODE_MODE=local nodemon --exec ts-node --watch src --delay 1 src/index.ts",
    "win:prod": "cross-env NODE_MODE=production ts-node src/index.ts",
    "win:dev": "npm run swagger & cross-env  NODE_MODE=developer nodemon --exec ts-node --watch src --delay 1 src/index.ts",
    "win:local": "npm run swagger & cross-env NODE_MODE=local nodemon --exec ts-node --watch src --delay 1 src/index.ts",
    "swagger": "swagger-cli bundle ./docs/swagger.yaml --outfile ./dist/swagger.yaml --type yaml",
    "build": "rm -rf ./dist & tsc -p tsconfig.json & npm run swagger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapsea-project/proxy-server-template.git"
  },
  "author": "devbyul-mapsea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapsea-project/proxy-server-template/issues"
  },
  "homepage": "https://github.com/mapsea-project/proxy-server-template#readme",
  "dependencies": {
    "@types/redis": "^4.0.11",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "nodemon": "^3.0.1",
    "redis": "^4.6.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-status-monitor": "^1.3.0",
    "@types/morgan": "^1.9.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express-status-monitor": "^1.3.4",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "prettier": "^3.0.0",
    "swagger-cli": "^4.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "yamljs": "^0.3.0"
  }
}
